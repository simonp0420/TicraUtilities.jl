// Main reflector stuff:
main_reflector  reflector 
 (
   coor_sys   : ref(main_refl_coord_sys),
   surface    : ref(main_surface_paraboloid), 
   rim        : ref(main_rim)
 )

main_surface_paraboloid  paraboloid 
 (
   vertex : struct(x: 0 m, y: 0 m, z: 0 m), 
   focal_length :   148.0000 in
 )

main_rim  elliptical_rim 
 (
   centre  : struct(x:    80.0000 in, y: 0.0 in),
