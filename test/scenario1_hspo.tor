// Reflectors and subreflectors
east_main_reflector  reflector  
(
  coor_sys         : ref(east_main_reflector_coor_sys),
  surface          : ref(east_parent_parabola),
  rim              : ref(harris_9m_rim),
  distortions      : sequence(ref(east_main_zernike_surface))
)
 
east_parent_parabola  paraboloid  
(
  focal_length     : 225.0 in
)
 
