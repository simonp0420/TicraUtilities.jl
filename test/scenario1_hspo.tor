(
  file_name        : east_main_reflector.zer,
  mode_unit        : in,
  centre           : struct(x: 214.0 in, y: 0.0 in),
  half_axis        : struct(x: 225.0 in, y: 225.0 in),
  max_m_mode_index : 2,
  max_n_mode_index : 2
)
 
